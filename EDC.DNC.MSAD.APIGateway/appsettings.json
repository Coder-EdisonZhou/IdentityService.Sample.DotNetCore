{
  "Swagger": {
    "DocName": "CAS.APIGateway",
    "Version": "v1",
    "Title": "API Gateway Service",
    "ServiceDocNames": "clientservice,productservice,noticeservice" // Swagger doc name list
  },
  "IdentityService": {
    "Uri": "http://localhost:5100",
    "UseHttps": false,
    "ApiSecrets": {
      "clientservice": "clientsecret",
      "productservice": "productsecret"
    }
  },
  "ServiceDiscovery": {
    "Host": "192.168.80.100", // Consul Service IP
    "Port": 8500 // Consul Service Port
  },
  "IdentityServer": {
    "Host": "192.168.80.1", // Identity Service IP
    "Port": 5100 // Identity Service Port
  },
  "TracingCenter": {
    "Uri": "http://localhost:9618", // Tracing Center Address
    "Name": "API Gateway" // Display Name
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
