{
  "Swagger": {
    "DocName": "CAS.APIGateway",
    "Version": "v1",
    "Title": "API Gateway Service",
    "ServiceDocNames": "clientservice,productservice,noticeservice" // Swagger doc name list
  },
  "IdentityService": {
    "Uri": "http://localhost:5100",
    "UseHttps": false,
    "ApiSecrets": {
      "clientservice": "clientsecret",
      "productservice": "productsecret"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
